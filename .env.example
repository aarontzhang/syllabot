# OpenAI API Key
# Get this from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Google Calendar API Credentials
# Get these from: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=https://your-app-name.onrender.com/oauth2callback

# Google Refresh Token (generated after first authorization)
# Leave empty initially - will be generated when you visit https://your-app-name.onrender.com/auth
GOOGLE_REFRESH_TOKEN=

# Port (for production, will be set automatically by hosting platform)
PORT=3000
